<%- include('partials/header.ejs'); %>
<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <%- include('partials/app-name.ejs'); %>
                </div>
                <div class="clearfix"></div>
                <%- include('partials/menu-profile.ejs'); %>
                <br>
                <%- include('partials/nav.ejs'); %>
                <%- include('partials/sidebar-footer.ejs'); %>
            </div>
        </div>
        <%- include('partials/top-nav.ejs'); %>
        <div class="right_col" role="main">
            <div>
                <div class="page-title">
                    <div class="title_left">
                        <h3>Ingest Packages</h3>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <div class="clearfix"></div>
                            </div>
                            <div id="ping"></div>
                            <div class="x_content">
                                <div id="message"></div>
                                <h4>Package(s) are ingested into Archivematica/DuraCloud and package data is imported into DigitalDU Repository</h4><br>
                                <table id="import-table" class="table table-striped table-bordered bulk_action">
                                    <thead>
                                    <tr>
                                        <th>Collection folders</th>
                                        <th>Number of Packages</th>
                                    </tr>
                                    </thead>
                                    <tbody id="packages"></tbody>
                                </table>
                                <div class="ingest-button"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include('partials/footer.ejs'); %>
    </div>
    <script src="<%=host%>libs/jquery/dist/jquery.min.js"></script>
    <script src="<%=host%>libs/axios-2.x/dist/axios.min.js"></script>
    <script src="<%=host%>libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="<%=host%>libs/nprogress/nprogress.js"></script>
    <script src="<%=host%>libs/dompurify/purify.min.js"></script>
    <script src="<%=host%>assets/js/theme_sidebar.js"></script>
    <script src="<%=host%>app/dom.module.js"></script>
    <script src="<%=host%>app/http.module.js"></script>
    <script src="<%=host%>app/helper.module.js"></script>
    <script src="<%=host%>app/ingest.module.js"></script>
    <script>ingestModule.init();</script>
</body>
</html>