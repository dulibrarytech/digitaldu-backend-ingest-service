<%- include('partials/header.ejs'); %>
<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <%- include('partials/app-name.ejs'); %>
                </div>
                <div class="clearfix"></div>
                <%- include('partials/menu-profile.ejs'); %>
                <br>
                <%- include('partials/nav.ejs'); %>
                <%- include('partials/sidebar-footer.ejs'); %>
            </div>
        </div>
        <%- include('partials/top-nav.ejs'); %>
        <div class="right_col" role="main">
            <div>
                <div class="page-title">
                    <div class="title_left">
                        <h1>Digital Preservation Jobs</h1>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <h2>Jobs History</h2>
                                <div class="clearfix"></div>
                            </div>
                            <div id="ping"></div>
                            <div class="x_content">
                                <div id="message" role="alert" aria-live="polite"></div>
                                <table id="jobs-history-table"
                                       class="table table-striped table-bordered bulk_action"
                                       role="grid"
                                       aria-label="Jobs History Table"
                                       style="visibility: hidden;">
                                    <thead>
                                    <tr>
                                        <th scope="col">Job ID</th>
                                        <th scope="col">Job Type</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Collection Folder</th>
                                        <th scope="col">Packages</th>
                                        <th scope="col">Job Run By</th>
                                        <th scope="col">Date</th>
                                    </tr>
                                    </thead>
                                    <tbody id="jobs-history"></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/repo/static/libs/jquery/dist/jquery.min.js"></script>
